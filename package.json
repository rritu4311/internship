{
  "name": "onlyinternship.in",
  "version": "0.1.0",
  "private": true,
  "description": "A modern, responsive internship portal for MBA students and employers",
  "keywords": [
    "internships",
    "mba",
    "career",
    "jobs",
    "students",
    "employers",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "OnlyInternship.in Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/lib/db-seed.ts",
    "db:init": "tsx src/lib/db-init.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "vercel-build": "next build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "@stripe/stripe-js": "^2.1.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "mongodb": "^6.19.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.5.2",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.5",
    "prisma": "^5.0.0",
    "rate-limiter-flexible": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "recharts": "^2.8.0",
    "sharp": "^0.32.6",
    "stripe": "^13.0.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.0.0",
    "@playwright/test": "^1.38.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.9",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "playwright": "^1.38.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^0.34.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/onlyinternship.in.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/onlyinternship.in/issues"
  },
  "homepage": "https://onlyinternship.in",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
